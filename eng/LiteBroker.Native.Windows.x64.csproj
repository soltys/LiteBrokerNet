<Project Sdk="Microsoft.NET.Sdk">
	<PropertyGroup>
		<TargetFramework>net7.0-windows</TargetFramework>
		<NoBuild>true</NoBuild>
		<Platforms>x64</Platforms>
		<GeneratePackageOnBuild>True</GeneratePackageOnBuild>
		<NuspecFile>LiteBroker.Native.Windows.x64.nuspec</NuspecFile>
		<NuspecProperties></NuspecProperties>
		<NuspecBasePath>../deps/LiteBroker/build</NuspecBasePath>
		<SkipCompilerExecution>true</SkipCompilerExecution>
		<CopyBuildOutputToOutputDirectory>false</CopyBuildOutputToOutputDirectory>
		<PackageOutputPath>../_Result/NuGet</PackageOutputPath>
		<NuspecProperties>target_file=$(MSBuildThisFileDirectory)LiteBroker.Native.Windows.x64.targets</NuspecProperties>
	</PropertyGroup>
</Project>
